mamba install pymeep=*=mpi_mpich_* -y
mamba install slepc4py=*=complex* -y
pip install -e .[tidy3d]
pip install jax jaxlib
pip install --upgrade "protobuf<=3.20.1"
pip install femwell
pip install scikit-fem[all] --upgrade

