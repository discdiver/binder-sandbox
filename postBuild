jupyter serverextension enable --py nbgitpuller --sys-prefix

git clone https://github.com/gdsfactory/ubc.git --shallow-exclude=gh-pages
git clone https://github.com/gdsfactory/gdsfactory.git --shallow-exclude=gh-pages
git clone https://github.com/gdsfactory/skywater130.git --shallow-exclude=gh-pages

ln -s  $HOME/gdsfactory/docs/notebooks notebooks 
pip install -e ubc
pip install -e skywater130
